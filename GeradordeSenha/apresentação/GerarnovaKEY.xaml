<Window x:Class="GeradordeSenha.apresentação.GerarnovaKEY"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeradordeSenha.apresentação"
        mc:Ignorable="d"
        Title="GerarnovaKEY" Height="180" Width="265" Background="#FFE6E6E6">
    <Grid>
        <Button x:Name="btn_GerarKEYAleatoria" Content="Gerar KEY aleatoria" HorizontalAlignment="Left" Margin="41,18,0,0" VerticalAlignment="Top" Width="62" Height="14" FontSize="6" FontWeight="Bold"/>
        <TextBlock x:Name="txb_KEYAleatoria" HorizontalAlignment="Left" Margin="108,18,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="115" Height="14" FontSize="10"/>
        <Label Content="Criar sua KEY" HorizontalAlignment="Left" Margin="41,68,0,0" VerticalAlignment="Top" Height="18" Width="48" FontSize="6" FontWeight="Bold"/>
        <TextBox HorizontalAlignment="Left" Margin="94,72,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Height="10" FontSize="6"/>
        <Label Content="OU" HorizontalAlignment="Center" Margin="0,42,0,0" VerticalAlignment="Top" FontSize="8"/>
        <TextBlock HorizontalAlignment="Left" Margin="41,132,0,0" TextWrapping="Wrap" Text="Se voce criar uma nova KEY todas as senhas que estavam salvas na antiga KEY irão ser apagadas" VerticalAlignment="Top" FontSize="6" FontWeight="Bold" Width="138"/>
        <Label Content="ATENÇÃO" HorizontalAlignment="Left" Margin="38,106,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="212,130,0,0" VerticalAlignment="Top" Width="26"/>

    </Grid>
</Window>
